
class Test
{
	int y=222;  //instance field

	void method1()
	{    int x=111;  //local variable
	     System.out.println("M1 x : "+x); 
		 System.out.println("M2 y : "+y); }

	void method2()
	{ System.out.println("M2 y : "+y); }

    public static void main(String args[])
	{
		Test t=new Test();
		         t.method1();
				 t.method2();
		System.out.println("Main y : "+t.y);
	}
}